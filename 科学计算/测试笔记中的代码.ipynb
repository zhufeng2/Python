   "source": [
    "from sympy import *\r\n",
    "#这个positive=true相当重要，正整数\r\n",
    "r=symbols('r',positive=true)\r\n",
    "x,y=symbols('x,y')\r\n",
    "circle=2*integrate(sqrt(r**2-x**2),(x,-r,r))\r\n",
    "circle_x=circle.subs(r,sqrt(r**2-x**2))\r\n",
    "v=integrate(circle_x,(x,-r,r))\r\n"
   ]
  }
 ]
